<link href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
<link href="//cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
<link rel="stylesheet" type="text/css" href="/css/demo.css">
<link rel="stylesheet" href="/css/typo.css">
<style type="text/css">
    .our-team {
        /*padding: 30px 0 40px;*/
        background: #f7f5ec;
        text-align: center;
        overflow: hidden;
        position: relative;
        border: 1px solid #110;
    }

    .our-team .pic {
        display: inline-block;
        /*width: 130px;*/
        /*height: 130px;*/
        /*margin-bottom: 50px;*/
        z-index: 1;
        position: relative;
    }

    .our-team .pic:before {
        content: "";
        width: 100%;
        height: 0;
        /*border-radius: 50%;*/
        background: #000000;
        position: absolute;
        bottom: 135%;
        right: 0;
        left: 0;
        opacity: 0.2;
        transform: scale(3);
        transition: all 0.3s linear 0s;
    }

    .our-team:hover .pic:before {
        height: 100%;
    }

    .our-team .pic:after {
        content: "";
        width: 100%;
        height: 100%;
        /*border-radius: 50%;*/
        background: #000000;
        position: absolute;
        top: 0;
        left: 0;
        z-index: -1;
    }

    .our-team .pic img {
        width: 100%;
        height: auto;
        transform: scale(1);
        transition: all 0.9s ease 0s;
    }

    .our-team:hover .pic img {
        box-shadow: 0 0 0 14px #f7f5ec;
        transform: scale(0.7);
    }

    .our-team .team-content {
        margin-bottom: 30px;
    }

    .our-team .title {
        font-size: 22px;
        font-weight: 700;
        color: #4e5052;
        letter-spacing: 1px;
        text-transform: capitalize;
        margin-bottom: 5px;
    }

    .our-team .post {
        display: block;
        font-size: 15px;
        color: #4e5052;
        text-transform: capitalize;
    }

    .our-team .social {
        width: 100%;
        padding: 0;
        margin: 0;
        background: #000;
        position: absolute;
        bottom: -100px;
        left: 0;
        transition: all 0.5s ease 0s;
    }

    .our-team:hover .social {
        bottom: 0;
    }

    .our-team .social li {
        display: inline-block;
    }

    .our-team .social li a {
        display: block;
        padding: 10px;
        font-size: 17px;
        color: #fff;
        transition: all 0.3s ease 0s;
    }

    .our-team .social li a:hover {
        color: #110;
        background: #f7f5ec;
    }

    @media only screen and (max-width: 990px) {
        .our-team {
            margin-bottom: 30px;
        }
    }</style>

<body class="is-loading">
<!-- Wrapper 外包 s-->
<div id="wrapper" class="fade-in">
    <!-- Intro 头部显示 s -->
    <!-- Intro 头部显示 e -->
    <!-- Header 头部logo start -->
    <%- partial('_partial/headerlogo') %>
    <!-- Nav 导航条 start -->
    <%- partial('_partial/nav') %>
    <div id="main">
        <div class="post_page_title_img"
             style="height: 25rem;background-image: url(<%= page.thumbnail %>);background-position: center; background-repeat:no-repeat; background-size:cover;-moz-background-size:cover;overflow:hidden;">
            <a href="#" style="padding: 4rem 4rem 2rem 4rem ;"><h2><%= page.title %></h2></a>
        </div>
        <% if (site.data.case) { %>
            <% for (var i in site.data.case) { %>
                <div class="typo" style="padding:1rem 3rem;border-top: none;">
                    <h2><%= site.data.case[i].name %></h2>
                </div>
                <div class="demo">
                    <div class="container" style="padding:1rem 3rem">
                        <div class="row">
                            <% if (site.data.case[i].items) { %>
                                <% for (var j in site.data.case[i].items) { %>
                                    <% if (site.data.case[i].name !== '同声传译') { %>
                                    <div class="col-md-3 col-sm-6" style="margin-top: 1rem;">
                                    <% } %>
                                    <% if (site.data.case[i].name === '同声传译') { %>
                                    <div class="col-md-6 col-sm-12" style="margin-top: 1rem;">
                                    <% } %>
                                        <div class="our-team">
                                            <div class="pic">
<!--                                                <% if (site.data.case[i].items[j].link != null) { %>-->
<!--                                                    <a href="<%= site.data.case[i].items[j].link %>">-->
<!--                                                        <img src="<%= site.data.case[i].items[j].avatar %>">-->
<!--                                                    </a>-->
<!--                                                <% } %>-->
                                            </div>
                                            <span class="title" style="white-space: nowrap"><%= j %></span>
                                        </div>
                                    </div>
                                <% } %>
                            <% } %>
                        </div>
                    </div>
                </div>
            <% } %>
        <% } %>
    </div>
    <!-- Copyright 版权 start -->
    <%- partial('_partial/copyright') %>
</div>
</body>