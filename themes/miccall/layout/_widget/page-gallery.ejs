<body class="is-loading-0 is-loading-1 is-loading-2">
	<!-- Main -->
	<div id="gallery_main">

		<!-- Header -->
		<header id="gallery_header">
			<h1><%- config.title %></h1>
			<p><%-theme.Gallery.description %></p>
			<ul class="gallery_icons">
				<% for (var i in theme.Nav.icon) { %>
				<li>
					<a title="<%= i %>" href="<%- url_for(theme.Nav.icon[i]) %>" target="_blank" rel="noopener">
						<i class="icon fa fa-<%= i %>"></i>
					</a>
				</li>
				<% } %>
			</ul>
		</header>

		<!-- Thumbnail -->
		<section id="gallery_thumbnails">
			<% if (site.data.gallery) { %>
			<% for (var i in site.data.gallery) { %>
			<article>
				<a class="thumbnail" href="<%= site.data.gallery[i].full_link %>" data-position="left center"><img
						src="<%= site.data.gallery[i].full_link %>" alt="" /></a>
				<h2><%= i %></h2>
				<p><%= site.data.gallery[i].descr %></p>
			</article>
			<% } %>
			<% } %>

		</section>
	</div>
	<%- partial('_partial/gallery_js', {}, {cache: true}) %>

</body>